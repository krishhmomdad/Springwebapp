pipeline{
agent any 

stages{
    stage('Checkout'){
         steps{
         git credentialsId: 'myjenkins', url: 'https://github.com/krishhmomdad/Springwebapp.git'
         } 
       }
    stage('Build'){
         steps{
         	sh 'mvn clean package'
         }
       }  
               
    stage('Deploy')
      {
      	steps{
        echo 'deploying'
        //sh 'ansible-playbook demo.yml'	
       	        }
            }   
        }     
 }
